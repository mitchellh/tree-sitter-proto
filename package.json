  },
  "keywords": [
    "tree-sitter",
    "proto"
  ],
  "author": "Mitchell Hashimoto <xmitchx@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.19.1"
  },
  "tree-sitter": [
    {
      "scope": "source.proto",
      "file-types": [
        "proto"
      ]
    }
  ]
}
